/* components/searchbar/searchbar.wxss */

.searchbar {
  background-color: #41be57;
  padding: 20rpx;
}

.search-navigator {
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx;
  background-image: url("http://m.qpic.cn/psb?/V11mcXK244wGyf/yMItdq9hoWYkxpB9AzWXft3zld1rKTifngEPLirA4dg!/b/dAgBAAAAAAAA&bo=yADIAAAAAAADByI!&rf=viewer_4");
  background-position: center;
  background-repeat: no-repeat;
  background-size: 8%;
}

.search-input-group{
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 10rpx 20rpx;
}

.search-input{
  /* min-height: 40rpx; */
  height: 40rpx;
  font-size: 14px;
}